{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/classic-editor": "1.6",
    "wpackagist-plugin/disable-emojis": "1.7.2",
    "wpackagist-plugin/query-monitor": "3.6.7",
    "wpackagist-plugin/zero-spam": "5.0.12",
    "wpackagist-plugin/wp-to-twitter": "3.5.2",
    "wpackagist-plugin/webmention": "4.0.5",
    "wpackagist-plugin/semantic-linkbacks": "3.10.4",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/indieweb-post-kinds": "3.5.6",
    "wpackagist-plugin/indieweb": "3.4.4",
    "wpackagist-plugin/jsonfeed": "1.4.1",
    "wpackagist-plugin/micropub": "2.2.3",
    "wpackagist-plugin/untappd-checkins-widget": "1.3.3",
    "wpackagist-plugin/wp-search-with-algolia": "1.7.0",
    "wpackagist-plugin/custom-post-type-ui": "1.9.1",
    "wpackagist-plugin/syndication-links": "4.3.3",
    "wpackagist-plugin/keyring-social-importers": "2.0",
    "wpackagist-plugin/keyring": "3.0",
    "wpackagist-plugin/avatar-privacy": "2.5.1",
    "wpackagist-plugin/indieauth": "3.6.0",
    "wpackagist-plugin/ninja-forms": "3.5.4",
    "wpackagist-plugin/slide": "0.0.39",
    "wpackagist-plugin/gutenberg": "10.5.0",
    "wpackagist-plugin/carbon-offset": "1.0.5",
    "wpackagist-plugin/cmb2": "2.9.0",
    "wpackagist-plugin/autodescription": "4.1.3",
    "wpackagist-plugin/ewww-image-optimizer": "6.1.2"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
