{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/classic-editor": "1.5",
    "wpackagist-plugin/disable-emojis": "1.7.2",
    "wpackagist-plugin/query-monitor": "3.4.0",
    "wpackagist-plugin/zero-spam": "3.1.1",
    "wpackagist-plugin/wp-to-twitter": "3.4.3",
    "wpackagist-plugin/wp-piwik": "1.0.22",
    "wpackagist-plugin/wordpress-seo": "12.2",
    "wpackagist-plugin/creative-commons-configurator-1": "1.8.27",
    "wpackagist-plugin/webmention": "4.0.0",
    "wpackagist-plugin/semantic-linkbacks": "3.9.3",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/indieweb-post-kinds": "3.3.3",
    "wpackagist-plugin/indieweb": "3.4.1",
    "wpackagist-plugin/jsonfeed": "1.3.0",
    "wpackagist-plugin/micropub": "2.0.11",
    "wpackagist-plugin/untappd-checkins-widget": "1.3.2",
    "wpackagist-theme/sempress": "1.5.12",
    "wpackagist-plugin/wp-search-with-algolia": "1.0.0",
    "wpackagist-plugin/custom-post-type-ui": "1.6.2",
    "wpackagist-plugin/syndication-links": "4.1.3",
    "wpackagist-plugin/keyring-social-importers": "2.0",
    "wpackagist-plugin/keyring": "2.0",
    "wpackagist-plugin/avatar-privacy": "2.3.1",
    "wpackagist-plugin/indieauth": "3.4.2",
    "wpackagist-plugin/ninja-forms": "3.4.20"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
