{
  "name": "tw2113/apiratelifeforme",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/classic-editor": "1.6.2",
    "wpackagist-plugin/disable-emojis": "1.7.4",
    "wpackagist-plugin/query-monitor": "3.8.2",
    "wpackagist-plugin/zero-spam": "5.2.13",
    "wpackagist-plugin/wp-to-twitter": "3.5.5",
    "wpackagist-plugin/webmention": "4.0.8",
    "wpackagist-plugin/semantic-linkbacks": "3.11.0",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/indieweb-post-kinds": "3.5.12",
    "wpackagist-plugin/indieweb": "3.4.6",
    "wpackagist-plugin/jsonfeed": "1.4.2",
    "wpackagist-plugin/micropub": "2.3.1",
    "wpackagist-plugin/untappd-checkins-widget": "1.3.3",
    "wpackagist-plugin/wp-search-with-algolia": "2.1.0",
    "wpackagist-plugin/custom-post-type-ui": "1.10.2",
    "wpackagist-plugin/syndication-links": "4.3.10",
    "wpackagist-plugin/keyring-social-importers": "2.0",
    "wpackagist-plugin/keyring": "3.0",
    "wpackagist-plugin/avatar-privacy": "2.5.2",
    "wpackagist-plugin/indieauth": "4.2.0",
    "wpackagist-plugin/ninja-forms": "3.6.7",
    "wpackagist-plugin/slide": "0.0.39",
    "wpackagist-plugin/gutenberg": "12.6.1",
    "wpackagist-plugin/carbon-offset": "1.0.6",
    "wpackagist-plugin/cmb2": "2.10.1",
    "wpackagist-plugin/autodescription": "4.2.3",
    "wpackagist-plugin/ewww-image-optimizer": "6.4.1",
    "wpackagist-plugin/wp-graphql": "1.6.12"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
