{
  "name": "tw2113/apiratelifeforme",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/antispam-bee": "2.11.5",
    "wpackagist-plugin/autodescription": "5.0.4",
    "wpackagist-plugin/autopost-to-mastodon": "3.7",
    "wpackagist-plugin/avatar-privacy": "2.7.0",
    "wpackagist-plugin/carbon-offset": "1.0.6",
    "wpackagist-plugin/classic-editor": "1.6.3",
    "wpackagist-plugin/cmb2": "2.10.1",
    "wpackagist-plugin/custom-post-type-ui": "1.15.1",
    "wpackagist-plugin/disable-emojis": "1.7.6",
    "wpackagist-plugin/ewww-image-optimizer": "7.2.3",
    "wpackagist-plugin/gutenberg": "17.6.0",
    "wpackagist-plugin/indieauth": "4.4.2",
    "wpackagist-plugin/indieweb": "4.0.2",
    "wpackagist-plugin/indieweb-post-kinds": "3.7.1",
    "wpackagist-plugin/jsonfeed": "1.4.5",
    "wpackagist-plugin/keyring": "3.0",
    "wpackagist-plugin/keyring-social-importers": "2.0",
    "wpackagist-plugin/micropub": "2.3.3",
    "wpackagist-plugin/ninja-forms": "3.7.2",
    "wpackagist-plugin/post-type-switcher": "3.2.2",
    "wpackagist-plugin/query-monitor": "3.15.0",
    "wpackagist-plugin/semantic-linkbacks": "3.12.0",
    "wpackagist-plugin/slide": "0.0.39",
    "wpackagist-plugin/syndication-links": "4.4.15",
    "wpackagist-plugin/untappd-checkins-widget": "1.3.3",
    "wpackagist-plugin/webmention": "5.2.3",
    "wpackagist-plugin/wp-graphql": "1.20.0",
    "wpackagist-plugin/wp-search-with-algolia": "2.7.0",
    "wpackagist-plugin/wp-uf2": "1.1.0"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    },
    "sort-packages": true
  }
}
